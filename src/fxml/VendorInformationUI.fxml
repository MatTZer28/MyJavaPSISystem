                  </HBox>
                  <HBox spacing="10.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label alignment="CENTER_RIGHT" contentDisplay="TOP" prefHeight="20.0" prefWidth="90.0" text="備註" textFill="#313f4d">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                        <TextField fx:id="TextField_comment" editable="false" prefHeight="23.0" prefWidth="389.0" />
                     </children>
                  </HBox>
               </children>
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
